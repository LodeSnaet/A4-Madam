{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "5.8.13.2",
    "craftcms/commerce": "5.4.4",
    "craftcms/commerce-mollie": "^4.4.0",
    "lode-snaet/craft-icon-picker": "@dev",
    "nystudio107/craft-seomatic": "5.1.16",
    "nystudio107/craft-vite": "5.0.1",
    "servd/craft-asset-storage": "4.1.2",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "php-http/discovery": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    },
    {
      "type": "vcs",
      "url": "https://github.com/LodeSnaet/IconPicker.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "modules\\mollie\\": "modules/mollie/"
    }
  }
}
